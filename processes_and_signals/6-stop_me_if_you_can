#!/usr/bin/env bash
# This is script 6th
pid=$(pgrep -f "4-to_infinity_and_beyond")

if [[ -z "$pid" ]]; then
    echo "Process not found"
    exit 1
fi

tty=$(readlink /proc/"$pid"/fd/0 2>/dev/null)

if [[ -z "$tty" ]]; then
    echo "Could not find terminal for process $pid"
    exit 1
fi

echo -ne '\x03' > "$tty"
