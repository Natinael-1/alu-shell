#!/usr/bin/env bash
# This is script 6th
for pid in /proc/[0-9]*; do
    cmd_path="$pid/cmdline"

    if [[ -r "$cmd_path" ]]; then
        cmd=$(tr '\0' ' ' < "$cmd_path")
        if echo "$cmd" | grep -q "./4-to_infinity_and_beyond"; then
            tty=$(readlink "$pid/fd/0" 2>/dev/null)

            if [[ -n "$tty" ]]; then
                echo -ne '\x03' > "$tty"
            fi
        fi
    fi
done
